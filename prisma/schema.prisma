generator client {
  provider = "prisma-client-js"
  //output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum GenderType {
  male
  female
  other
}

model User{
  id String @id @default(uuid())
  name String 
  email String @unique
  password String
  gender  GenderType
  address String
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt

  @@map("user")
}
